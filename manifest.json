{
  "manifest_version": 2,
  "name": "hello world",
  "version": "1.0",
  "description": "A hello world extension",
  "permissions": ["tabs"],
  "content_security_policy": "script-src 'self' 'sha256-RmaO0PtNN18LOnKE0JnUJxa0aa2Vqq9CTD0b1o+0e/4='",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "browser_action":{
    "default_title": "Hey!",
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "background": {
  "scripts": ["jquery-3.2.1.min.js", "background.js"]
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  }
}
